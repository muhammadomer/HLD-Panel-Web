@model Hld.WebApplication.ViewModel.Expected_Delivery_Shipped_POViewModel
@using Hld.WebApplication.Models

<div class="container">
    <form asp-action="AddDate" id="form1" role="form">
        <div class="form-group row">
            <label class="col-lg-4 col-sm-4 control-label font-weight-bold mr-2">Expected Delivery</label>
            <div class="col-lg-9">
                <input type="text" style="display:none" value="@Model.ShipmentId" name="ShipmentId" />          
                <input type="date" name="ExpectedDelivery" class="form-control" id="datePicker"/>
                <span asp-validation-for="ExpectedDelivery" class="text-danger"></span>
            </div>
           
        </div>

        <div class="form-group">
            <input type="button" class="btn btn-primary"id="btnSaveBrand" value="Submit" onclick="return SubmitForm();" />
        </div>

    </form>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

