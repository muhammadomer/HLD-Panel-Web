@model Hld.WebApplication.ViewModel.BestBuyProductZincViewModel
@using Hld.WebApplication.Models

<div class="container">
    <form asp-action="AddUpdateBrand" id="form1" role="form">
        <div class="form-group row">
            <label class="col-lg-3 col-sm-3 control-label font-weight-bold mr-2">ZASIN-CA</label>
            <div class="col-lg-9">
                @Html.HiddenFor(e => e.BestBuyProductZincID)
                <input asp-for="ZincAsinCA" id="BrandNameId" class="form-control" />
                <span asp-validation-for="ZincAsinCA" class="text-danger"></span>
            </div>
            <span id="IsExists"></span>
        </div>
        <div class="form-group row">
            <label class="col-lg-3 col-sm-3 control-label font-weight-bold mr-2">Priority</label>
            <div class="col-lg-9">
                
                <input asp-for="Priority" id="BrandNameId" class="form-control" />
                <span asp-validation-for="Priority" class="text-danger"></span>
            </div>
            <span id="IsExists"></span>
        </div>

        <div class="form-group">
            <input type="button" class="btn btn-light" disabled="true" id="btnSaveBestBuyProductZinc" value="Submit" onclick="return SubmitForm();" />
        </div>

    </form>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

