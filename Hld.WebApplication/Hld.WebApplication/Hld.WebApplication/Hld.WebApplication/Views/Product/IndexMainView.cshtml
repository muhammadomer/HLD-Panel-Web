                alert("please select only one option");
            }
            else if ($(".checkboxSkuForUpdating").is(":checked")) {

                $('#RemoveModalOpen').modal({
                    //backdrop: 'static',
                    //keyboard: false
                });
                var productSKU = "";
                $(document).find('.checkboxSkuForUpdating').each(function (index) {
                    var status = $(this).is(':checked');
