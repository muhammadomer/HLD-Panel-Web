                        </span>
                        @*<button class="ml-1 btn btn-primary" type="button" id="btnexportOrderget">Export</button>*@

                    </div>
                    <div class="row col-sm-12 mt-2">
                        <div class="p-0 mr-1" style="position:relative;width:160px">
                            <select id="multiselect" class="form-control mb-2 mr-sm-2 mt-2 active ASINListingRemoved" multiple="multiple" style="position:absolute">
                                <option value="Yes">Yes</option>
                                <option value="No">No</option>
                            </select>
                            <input type="hidden" name="ASINListingRemoved" id="ddlASINListingRemoved" />
                        </div>
                        <div class="tooltipactivebb">
                            <div class="p-0 mr-1 showclasss" style="position:relative;width:170px">
                                <select id="multiselect" class="form-control mb-2 mr-sm-2 mt-2 active BBPriceUpdateClass" multiple="multiple" style="position:absolute">
                                    <option value="1">In Last 7 Days</option>
                                    <option value="2">In Last 14 Days</option>
                                    <option value="3">In Last Month</option>
                                    <option value="4">In Last 3 Month</option>
