                        productSKU = $(this).closest('tr').find('#productSkuForUpdating').val();

                    }
                });

                var Url = '/Tag/GetTagforsku/';

                $("#selectedRemove").empty();

                $.ajax({
                    type: 'Get',
                    url: Url,
                    data: { sku: productSKU },
                    dataType: 'json',
                    traditional: true,
