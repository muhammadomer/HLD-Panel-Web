    .tooltipactivebb {
        position: relative;
        display: contents;
    }

        .tooltipactivebb .tooltiptexttbb {
            visibility: hidden;
            width: 400px;
            background-color: #555;
            color: #fff;
            text-align: center;
            border-radius: 6px;
            padding: 5px 0;
            text-align: left;
            padding-left: 10px;
            font-family: arial;
            position: absolute;
            z-index: 113;
            bottom: 52%;
            /* left: -245px; */
            margin-bottom: 17px;
            margin-left: 0px;
            opacity: 0;
            transition: opacity 0.3s;
        }

            .tooltipactivebb .tooltiptexttbb::after {
                content: "";
                position: absolute;
                top: 100%;
                left: 5%;
                margin-left: -5px;
                border-width: 5px;
                border-style: solid;
                border-color: #555 transparent transparent transparent;
            }

        .tooltipactivebb:hover .tooltiptexttbb {
            visibility: visible;
            opacity: 1;
        }

    .tooltipactivebbinternal {
        position: relative;
        display: contents;
    }

        .tooltipactivebbinternal .tooltiptexttbbinternal {
            visibility: hidden;
            width: 270px;
            background-color: #555;
            color: #fff;
            text-align: center;
            border-radius: 6px;
            padding: 5px 0;
            text-align: left;
            padding-left: 10px;
            font-family: arial;
            position: absolute;
            z-index: 113;
            bottom: 52%;
            /* left: -245px; */
            margin-bottom: 17px;
            margin-left: 0px;
            opacity: 0;
            transition: opacity 0.3s;
        }

            .tooltipactivebbinternal .tooltiptexttbbinternal::after {
                content: "";
                position: absolute;
                top: 100%;
                left: 5%;
                margin-left: -5px;
                border-width: 5px;
                border-style: solid;
                border-color: #555 transparent transparent transparent;
            }

        .tooltipactivebbinternal:hover .tooltiptexttbbinternal {
            visibility: visible;
            opacity: 1;
        }
    .taground {
        color: white;
        font-size: 10px;
        padding: 1px;
        border: none;
        border-radius: 3px;
        margin-right: 5px;
    }
</style>
<div>
    <div class="loader">
        <img src="~/Images/giffff.gif" />
    </div>
    <div class="mb-1 row" id="searchForm" style="margin-top: 60px;">

        <div class="col-sm-12">

            @if (Model.ASINS != "ALL" && !string.IsNullOrEmpty(Model.ASINS))
            {
                <div class="row col-12 ASINHide">
                    <span class="tagspan">ASIN Available Status :</span>

                    @if (Model.ASINS == "Yes")
                    {

                        <span style="padding-left:0px;" class="asin-status-text">Yes</span><span style="margin-left:3px"><input class="ASINCLASS" type="checkbox" value="@Model.ASINS" checked></span>
                    }
                    else
                    {
                        <span style="padding-left:0px;" class="asin-status-text">No</span><span style="margin-left:3px"><input class="ASINCLASS" type="checkbox" value="@Model.ASINS" checked></span>
                    }

                </div>
            }
            @if (Model.BBProductID != "ALL" && !string.IsNullOrEmpty(Model.BBProductID))
            {
                <div class="row col-12 SKUHide">
                    <span class="tagspan">Show SKUs Status :</span>

                    @if (Model.BBProductID == "Yes")
                    {

                        <span style="padding-left:0px;" class="sku-status-text">W BB Product ID</span><span style="margin-left:3px"><input class="SKUCLASS" type="checkbox" value="@Model.BBProductID" checked></span>
                    }
                    else
                    {
                        <span style="padding-left:0px;" class="sku-status-text">WO BB Product ID</span><span style="margin-left:3px"><input class="SKUCLASS" type="checkbox" value="@Model.BBProductID" checked></span>
                    }

                </div>
            }
            @if (Model.ApprovedUnitPrice != "ALL" && !string.IsNullOrEmpty(Model.ApprovedUnitPrice))
            {
                <div class="row col-12 APRHide">
                    <span class="tagspan">Approved Prices Status :</span>

                    @if (Model.ApprovedUnitPrice == "Yes")
                    {

                        <span style="padding-left:0px;" class="apr-status-text">Yes</span><span style="margin-left:3px"><input class="APRCLASS" type="checkbox" value="@Model.ApprovedUnitPrice" checked></span>
                    }
                    else
                    {
                        <span style="padding-left:0px;" class="apr-status-text">No</span><span style="margin-left:3px"><input class="APRCLASS" type="checkbox" value="@Model.ApprovedUnitPrice" checked></span>
                    }

                </div>
            }
            @if (Model.WHQStatus != "ALL" && !string.IsNullOrEmpty(Model.WHQStatus))
            {
                <div class="row col-12 WHQHide">
                    <span class="tagspan">WHQ Status :</span>

                    @if (Model.WHQStatus == "1")
                    {

                        <span style="padding-left:0px;" class="whq-status-text">WHQ > 0</span><span style="margin-left:3px"><input class="WHQCLASS" type="checkbox" value="@Model.WHQStatus" checked></span>
                    }
                    else
                    {
                       @*  <span style="padding-left:0px;" class="whq-status-text">WHQ <= 0</span> *@
                        <span style="margin-left:3px"><input class="WHQCLASS" type="checkbox" value="@Model.WHQStatus" checked></span>
                    }
                </div>
            }
            @if (Model.BBPriceUpdate != "ALL" && !string.IsNullOrEmpty(Model.BBPriceUpdate))
            {
                <div class="row col-12 BBPriceUpdateHide">
                    <span class="tagspan">BB Prices Last Update Status :</span>

                    @if (Model.BBPriceUpdate == "1")
                    {

                        <span style="padding-left:0px;" class="">Last 7 Days</span><span style="margin-left:3px"><input class="BBPriceUpdateCLASS" type="checkbox" value="@Model.BBPriceUpdate" checked></span>
                    }
                    else if (Model.BBPriceUpdate == "2")
                    {
                        <span style="padding-left:0px;" class="">Last 14 Days</span><span style="margin-left:3px"><input class="BBPriceUpdateCLASS" type="checkbox" value="@Model.BBPriceUpdate" checked></span>
                    }
                    else if (Model.BBPriceUpdate == "3")
                    {
                        <span style="padding-left:0px;" class="">Last Month</span><span style="margin-left:3px"><input class="BBPriceUpdateCLASS" type="checkbox" value="@Model.BBPriceUpdate" checked></span>
                    }
                    else if (Model.BBPriceUpdate == "4")
                    {
                        <span style="padding-left:0px;" class="">Last 3 Month</span><span style="margin-left:3px"><input class="BBPriceUpdateCLASS" type="checkbox" value="@Model.BBPriceUpdate" checked></span>
                    }
                    else if (Model.BBPriceUpdate == "5")
                    {
                        <span style="padding-left:0px;" class="">Last 6 Month</span><span style="margin-left:3px"><input class="BBPriceUpdateCLASS" type="checkbox" value="@Model.BBPriceUpdate" checked></span>
                    }

                    else
                    {
                        <span style="padding-left:0px;" class="">Last 1 Year</span><span style="margin-left:3px"><input class="BBPriceUpdateCLASS" type="checkbox" value="@Model.BBPriceUpdate" checked></span>
                    }
                </div>
            }

            @if (Model.dropshipstatusSearch != "ALL" && !string.IsNullOrEmpty(Model.dropshipstatusSearch))
            {
                <div class="row col-12 DROPHide">
                    <span class="tagspan">Dropship Status :</span>

                    @if (Model.dropshipstatusSearch == "1")
                    {

                        <span style="padding-left:0px;" class="drop-status-text">Dropship</span><span style="margin-left:3px"><input class="DROPCLASS" type="checkbox" value="@Model.dropshipstatusSearch" checked></span>
                    }
                    else
                    {
                        <span style="padding-left:0px;" class="drop-status-text">Dropship None</span><span style="margin-left:3px"><input class="DROPCLASS" type="checkbox" value="@Model.dropshipstatusSearch" checked></span>
                    }

                </div>
            }
            @if (Model.ASINListingRemoved != "ALL" && !string.IsNullOrEmpty(Model.ASINListingRemoved))
            {
                <div class="row col-12 LSRHide">
                    <span class="tagspan">ASIN Listing Removed Status :</span>

                    @if (Model.ASINListingRemoved == "Yes")
                    {

                        <span style="padding-left:0px;" class="">Yes</span><span style="margin-left:3px"><input class="LSRCLASS" type="checkbox" value="@Model.ASINListingRemoved" checked></span>
                    }
                    else
                    {
                        <span style="padding-left:0px;" class="">No</span><span style="margin-left:3px"><input class="LSRCLASS" type="checkbox" value="@Model.ASINListingRemoved" checked></span>
                    }
                </div>
            }
            @if (Model.BBInternalDescription != "ALL" && !string.IsNullOrEmpty(Model.BBInternalDescription))
            {
                <div class="row col-12 BBInternalHide">
                    <span class="tagspan">BB ASIN Internal Description Status :</span>

                    @if (Model.BBInternalDescription == "Yes")
                    {

                        <span style="padding-left:0px;" class="">Yes</span><span style="margin-left:3px"><input class="BBINTERNALCLASS" type="checkbox" value="@Model.BBInternalDescription" checked></span>
                    }
                    else
                    {
                        <span style="padding-left:0px;" class="">No</span><span style="margin-left:3px"><input class="BBINTERNALCLASS" type="checkbox" value="@Model.BBInternalDescription" checked></span>
                    }
                </div>
            }
            @if (Model.DSTag != "ALL" && !string.IsNullOrEmpty(Model.DSTag))
            {
                <div class="row col-12 TGHide">
                    <span class="tagspan">Tag Status :</span>


                    @foreach (var item in Model.DSTag.Split(","))
                    {

                        @switch (item)
                        {
                            case "1":
                                <label style="padding-left:0px;" value="@item" class="TG-status-text">DS</label> break;
                            case "2":
                                <span style="padding-left:0px;" value="@item" class="TG-status-text">WH</span> break;

                            case "3":
                                <span style="padding-left:0px;" value="@item" class="TG-status-text">Zinc</span> break;
                            case "4":
                                <span style="padding-left:0px;" value="@item" class="TG-status-text">HA</span> break;
                            case "5":
                                <span style="padding-left:0px;" value="@item" class="TG-status-text">PrAdj</span>break;

                        }
                    }<span style="margin-left:3px"><input class="TGCLASS" type="checkbox" value="" checked></span>




                </div>}
            <h6 style="color:cadetblue">Search Inventory</h6>
            <div class="card-body" style="padding:0">
